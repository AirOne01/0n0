{
  "name": "@0n0/ui",
  "version": "0.1.0",
  "description": "UI components for 0n0",
  "private": true,
  "type": "module",
  "repository": "https://github.com/AirOne01/0n0.git",
  "packageManager": "pnpm@8.15.4",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.js",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ],
    "./tailwind": {
      "require": "./src/tailwind.cjs"
    }
  },
  "files": [
    "src"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "peerDependencies": {
    "next": "*",
    "react": "^17.0.0 || ^18.0.0-0",
    "react-dom": "^17.0.0 || ^18.0.0-0"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.8",
    "clsx": "^1.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "typescript": "^5.1.6",
    "xo": "^0.58.0"
  }
}